{
    "plugins": ["cypress"],
    "extends": ["plugin:cypress/recommended"],
    "rules": {
      "jest/expect-expect": "off"
    },
    "eslint.workingDirectories": [
        "./front-end"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false,
        "babelOptions": {
            "presets": ["@babel/preset-react"],
            "parserOpts": {
                "plugins": ["jsx"]
              }
         }
    }
  }